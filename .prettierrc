{
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "objectPropertyNewline": true,
    "objectCurlyNewline": {
      "multiline": true,
      "minProperties": 2
    },
    "semi": false
  }
  